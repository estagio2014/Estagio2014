  Vcl.Buttons, Vcl.Mask;

type
  TfrmCadUsuario = class(TfrmCadPrincipal)
    Label5: TLabel;
    GroupBox1: TGroupBox;
    Label1: TLabel;
    Label2: TLabel;
    Label3: TLabel;
    Label4: TLabel;
    Label6: TLabel;
    Label7: TLabel;
    Label8: TLabel;
    edtId: TEdit;
    edtNome: TEdit;
    edtSenha: TEdit;
    edtUsuario: TEdit;
    cboNivelAcc: TComboBox;
    edtDataCad: TMaskEdit;
    edtConfSenha: TEdit;
    procedure FormShow(Sender: TObject);
  private
    { Private declarations }
